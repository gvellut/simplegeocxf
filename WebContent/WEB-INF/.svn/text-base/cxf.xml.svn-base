<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 					http://www.springframework.org/schema/beans/spring-beans.xsd
 					http://cxf.apache.org/jaxws
 					http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

	<bean id="mapServiceConfiguration"
		class="com.gaiadyne.opengeoreports.server.model.MapServiceConfiguration">
		<property name="mapServices">
			<map></map>
		</property>
	</bean>

	<jaxws:endpoint id="openGeoreports"
		implementor="#openGeoreportsService" address="/mapService" />

	<bean id="openGeoreportsService"
		class="com.gaiadyne.opengeoreports.server.service.OpenGeoreportsServiceImpl">
		<property name="mapServiceConfiguration"
			ref="mapServiceConfiguration" />
	</bean>

</beans>